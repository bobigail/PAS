<ligand-database>
    <ligand id="THREE_LETTER_ID"> <!-- <PDBx:three_letter_code> -->
        
        <name>NAME_IN_WORDS</name> <!-- ligand name <PDBx:name> -->
        <type>TYPE_OF_LIGAND</type> <!-- type of ligand <PDBx:type> -->
        <formula>CHEMICAL_FORMULA</formula> <!-- formula of ligand <PDBx:formula> -->
